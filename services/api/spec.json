			"$id": "schemas-op",
			"type": "string",
			"enum": [
				"~",
				"=",
				"!=",
				">=",
				">",
				"<=",
				"<",
				"IN",
				"NOT IN",
				"BETWEEN",
				"XBETWEEN"
			]
		},
