			"$id": "schemas-democracy_meta_branch",
			"description": "Meta rules for recursive structures",
			"type": "object",
			"patternProperties": {
				".*": {
					"oneOf": [
						{ "$ref": "schemas-democracy_meta_leaf" },
						{ "$ref": "schemas-democracy_meta_branch" }
					]
				}
			},
			"additionalProperties": false
		},
